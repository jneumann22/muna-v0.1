{
    "name": "Muna V0.1",
    "description": "Muna",
    "version": "0.1.0",
    "manifest_version": 2,
    "icons": {
        "16": "./icon-16x16.png",
        "32": "./icon-32x32.png",
        "48": "./icon-48x48.png",
        "128": "./icon-128x128.png"
    },
    "background": {
        "scripts": ["background.js"]
    },
    
    "browser_action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "tabs",
        "<all_urls>",
        "activeTab"
    ]
}